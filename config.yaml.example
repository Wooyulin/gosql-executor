database:
  type: "mysql"  # mysql, oracle, pgsql
  username: "your_username"
  password: "your_password"
  
  # MySQL DSN 示例
  # dsn: "{username}:{password}@tcp(localhost:3306)/dbname"
  
  # Oracle DSN 示例（使用 SID）
  # dsn: "oracle://{username}:{password}@host:1521/dbname?sid=sid"
  # Oracle DSN 示例（使用 Service Name）
  # dsn: "oracle://{username}:{password}@host:1521/service_name"
  
  # PostgreSQL DSN 示例
  # dsn: "postgres://{username}:{password}@localhost:5432/dbname?sslmode=disable"
  
  dsn: "your_database_connection_string_template"

output:
  directory: "./output"
  format: "csv"
  show_in_console: true
  save_to_file: true